    "css-loader": "3.2.0",
    "dotenv-webpack": "1.7.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-react-hooks": "1.7.0",
    "eslint-plugin-typescript": "0.14.0",
    "faker": "4.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
